<h1>Listing link_clicks</h1>

<table>
  <thead>
    <tr>
      <th>App link</th>
      <th>Ip adress</th>
      <th>Occurence counter</th>
      <th>Installed</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @link_clicks.each do |link_click| %>
      <tr>
        <td><%= link_click.app_link_id %></td>
        <td><%= link_click.ip_adress %></td>
        <td><%= link_click.occurence_counter %></td>
        <td><%= link_click.installed %></td>
        <td><%= link_to 'Show', link_click %></td>
        <td><%= link_to 'Edit', edit_link_click_path(link_click) %></td>
        <td><%= link_to 'Destroy', link_click, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Link click', new_link_click_path %>
